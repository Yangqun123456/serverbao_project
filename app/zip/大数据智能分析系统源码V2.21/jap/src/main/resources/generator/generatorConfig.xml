<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
	  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<properties resource="generator/generatorConfig.properties" /> 
    <!-- <properties url="file:////C:\Users\Reno\Desktop\JiangSuDev\trunk\wzdap\wzdap-parent\wzdap-biz\src\main\resources\generator\generatorConfig.properties"/> -->

	<!-- 生成代码的运行环境 -->
	<context id="Tables" targetRuntime="MyBatis3Simple" defaultModelType="flat">
		<plugin type="tk.mybatis.mapper.generator.MapperPlugin">
          <property name="mappers" value="tk.mybatis.mapper.common.Mapper"/>
        </plugin>
		
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>

		<!-- 设置注解信息 -->
		<commentGenerator type="com.cnshangquan.code.dao.mybatis.MyCommentGenerator">
			<!-- <property name="suppressDate" value="true"/> -->  
            <property name="suppressAllComments" value="false"/>
		</commentGenerator>

		<jdbcConnection driverClass="${jdbc.driverClassName}" connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- generate Model -->
		<javaModelGenerator targetPackage="${package.name}.${module.name}.domain" targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="false" />
		</javaModelGenerator>



		<!-- generate xml -->
		<!--<sqlMapGenerator targetPackage="${package.name}.${module.name}.persistence" targetProject=".\src\main\resources">-->
		<sqlMapGenerator targetPackage="${module.name}" targetProject=".\src\main\resources\mybatis">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- generate Mapper -->
		 <javaClientGenerator type="XMLMAPPER" targetPackage="${package.name}.${module.name}.mapper"
			targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator> 


		<table schema="" tableName="${table.name}" enableCountByExample="false" domainObjectName="${domain.object.name}" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <!-- 忽略列，不生成bean 字段 -->
             <!--<ignoreColumn column="create_by" domainObjectName="${domain.object.name}" />-->

        </table>
		<!--<table tableName="B_%" />-->


	</context>
</generatorConfiguration>
